{
    "manifest_version": 2,
  
    "name": "askualabs",
    "description": "This extension allows the user to change the background color of the current page.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Askualabs"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }